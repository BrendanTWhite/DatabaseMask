{
    "name": "brendantwhite/databasemask", 
    "description": "Mask data from Production using Faker to use safely elsewhere",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/brendantwhite/databasemask",
    "authors": [
        {
            "role": "Developer",
            "name": "Brendan T White",
            "email": "brendan@thespia.com"
        }
    ],
    "keywords": [
        "laravel",
        "database",
        "masking",
        "obfuscation",
        "privacy"
      ],    
    "autoload": {
        "psr-4": {
            "BrendanTWhite\\DatabaseMask\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrendanTWhite\\DatabaseMask\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [      
                "BrendanTWhite\\DatabaseMask\\DatabaseMaskServiceProvider"
            ],
            "aliases": {
                
            }
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support":  "^8.67|^9.0"
    },
    "suggest": {
        "spatie/laravel-db-snapshots": "Required for the dbm:backup and dbm:restore commands"
      },
        "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
        "require-dev": {
        "laravel/pint": "^1.2"
    }
}
